#!/bin/bash


if [ -e "$HOME/.tmux.conf" ]; then
    echo "1,开始备份\"$HOME/.tmux.conf\""
    sudo cp ~/.tmux.conf ./
fi

if [ -e "$HOME/.profile" ]; then
    echo "2,开始备份\"$HOME/.profile\""
    sudo cp ~/.profile ./
fi

if [ -e "$HOME/.bashrc" ]; then
    echo "3,开始备份\"$HOME/.bashrc\""
    sudo cp ~/.bashrc ./
fi


if [ -e "$HOME/.config/fish/config.fish" ]; then
    echo "4,开始备份\"$HOME/.config/fish/config.fish\""
    sudo cp ~/.config/fish/config.fish ./
fi

if [ -e "/etc/apache2/apache2.conf" ]; then
    echo "5,开始备份\"/etc/apache2/apache2.conf\""
    sudo cp /etc/apache2/apache2.conf ./
fi

if [ -e "$HOME/.config/nvim/vimrcs/basic.vim" ]; then
    echo "6,开始备份\"$HOME/.config/nvim/vimrcs/basic.vim\""
    sudo cp $HOME/.config/nvim/vimrcs/basic.vim ./
fi

